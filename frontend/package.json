    },
    "private": true,
    "scripts": {
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "lint": "ng lint",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "spell": "cspell \"**/*\"",
        "test": "ng test --watch=false --code-coverage",
        "test:watch": "ng test",
        "test:e2e": "ng e2e",
        "test:all": "npm run spell && npm run lint && npm run format:check && npm run test && npm run test:e2e",
        "ng": "ng",
        "c": "ng generate component",
        "s": "ng generate service",
        "a": "ng add"
    },
