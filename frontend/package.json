        "name": "Adri√°n Mezei",
        "email": "adrian.mezei.dev@gmail.com"
    },
    "private": true,
    "scripts": {
        "start": "ng serve --host 0.0.0.0",
        "build": "ng build",
        "build-prod": "ng build --prod",
        "lint": "ng lint",
        "format": "prettier --write .",
        "test": "ng test --watch=false --code-coverage",
        "test:watch": "ng test",
        "test:e2e": "ng e2e",
        "test:all": "npm run lint && npm run format && npm run test && npm run test:e2e",
        "ng": "ng",
        "c": "ng generate component",
        "s": "ng generate service services/backend-base/backend-base"
    },
