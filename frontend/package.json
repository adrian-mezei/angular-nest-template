{
    "name": "angular-nest-template-frontend",
    "description": "Angular template for further projects that use Angular as a frontend framework.",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/adrian-mezei-smart-home/angular-nest-template.git"
    },
